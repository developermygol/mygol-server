{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build-webapi",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/webapi/webapi.csproj"
            ],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "build-cli",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/mygolcli/mygolcli.csproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}